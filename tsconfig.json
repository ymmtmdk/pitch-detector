{
    "compileOnSave": false,
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "lib": [
            "dom",
            "es2015",
            "es2016"
        ],
        "target": "es2016",
        "module": "es2015",
        "moduleResolution": "node",
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "allowJs": true,
        "skipLibCheck": true
    }
}
